#Select the first 5 records from table saplane. Process the data using a DO N TIMES loop and display the planetype and seatsmax fields.

REPORT ZTABELADO.

DATA: lt_saplane TYPE STANDARD TABLE OF saplane,
      ls_saplane TYPE saplane.


SELECT *
  INTO TABLE lt_saplane
  FROM saplane.

DO 5 TIMES.

  READ TABLE lt_saplane INTO ls_saplane INDEX sy-index.
  WRITE: ls_saplane-planetype, ls_saplane-seatsmax.
  NEW-LINE.

ENDDO.
